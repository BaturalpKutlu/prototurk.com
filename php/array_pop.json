  "title": "PHP array_pop() Fonksiyonu",
  "name": "array_pop",
  "lang": "PHP",
  "description": "Dizinin sonundaki elemanı diziden çıkartır.",
  "syntax": "array_pop ( array $dizi ) : mixed",
  "params": [
    {
      "name": "$dizi",
      "type": "string",
      "description": "Değer alınacak dizi.",
      "required": true
    }
  ],
  "return": {
    "type": "mixed",
    "text": "Dizideki son elemanın tipine bağlı olarak geriye son elemanın değerini döndürür."
  },
  "compatibilities": [
    "php 4",
    "php 5",
    "php 7"
  ],
  "deprecated": false,
  "examples": [
    {
      "description": "Aşağıdaki örnekte dizinin son elemanını çıkartıyoruz.",
      "code": "<?php $depo = array('portakal','muz','elma','ahududu'); $meyve = array_pop($depo); print_r($depo); ?>"
    },
    {
      "description": "Ayrıca <code>array_pop()</code> geriye son elemanın değerinide döndürür.",
      "code": "<?php $depo = array('portakal','muz','elma','ahududu'); $meyve = array_pop($depo); echo $meyve; ?>"
    }
  ]
}
